{
  "name": "express-cart",
  "version": "1.1.18",
  "description": "A fully functioning Node.js shopping cart with Stripe, PayPal and Authorize.net payments.",
  "private": false,
  "scripts": {
    "start": "node app.js",
    "deploy": "gulp deploy",
    "testdata": "node lib/testdata.js",
    "test": "ava",
    "dev": "gulp watch",
    "lint": "eslint ./"
  },
  "ava": {
    "serial": true,
